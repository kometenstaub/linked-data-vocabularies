{
    "name": "linked-data-vocabularies",
    "version": "1.1.3",
    "description": "Add SKOS heading classification to YAML.",
    "main": "src/main.ts",
    "scripts": {
        "dev": "cross-env BUILD=dev node esbuild.mjs",
        "build": "cross-env BUILD=production node esbuild.mjs",
        "release": "standard-version",
        "lint": "npx eslint src/",
        "test": "jest",
        "format": "npx prettier --write src/"
    },
    "standard-version": {
        "t": "",
        "types": [
            {
                "type": "perf",
                "hidden": false,
                "section": "Performance"
            },
            {
                "type": "feat",
                "hidden": false,
                "section": "Features"
            },
            {
                "type": "fix",
                "hidden": false,
                "section": "Bug Fixes"
            }
        ],
        "releaseCommitMessageFormat": "Linked Data Vocabs Release: {{currentTag}}"
    },
    "keywords": [],
    "author": "kometenstaub",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^14.14.37",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.10.0",
        "cross-env": "^7.0.3",
        "esbuild": "^0.13.2",
        "esbuild-plugin-inline-worker": "^0.1.1",
        "eslint": "^8.7.0",
        "obsidian": "^1.1.1",
        "standard-version": "^9.3.1",
        "tslib": "^2.2.0",
        "typescript": "^4.2.4"
    },
    "dependencies": {
        "fuzzysort": "^1.1.4"
    }
}
